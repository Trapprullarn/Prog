package w8;

import java.util.Scanner;

public class GissaTal {
	public static void main(String[] args) {

		Scanner scanner = new Scanner(System.in);
		int close = 0;

		boolean running = true;
		while (running) {
			System.out.println("Patrik, döda mig");
			close++;
			close++;
			if (close == 3) {
				break;
			}
		}
	}
}
