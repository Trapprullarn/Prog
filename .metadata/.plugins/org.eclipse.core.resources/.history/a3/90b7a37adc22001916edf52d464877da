package w5;

import java.util.Arrays;

public class dice {

	public static void main(String[] args) {
		
		int[] kast = {0,0,0,0,0,0};
	
		for(int i = 0; i < 1000; i++) {
			int random = (int) (Math.random()*6);
			kast[random]++;
		}
		for (int i = 0; i < kast.length; i++)
		System.out.println("Antal " + (i + 1) + "or" + kast[i]);
	}
}
